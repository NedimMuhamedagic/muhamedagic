    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.3.1",
    "lint-staged": "^7.2.2",
    "prettier-eslint-cli": "^4.7.1",
    "redux-devtools": "^3.4.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.6"
