{
  "name": "nmuhamedagic",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "8.9.0"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode production --config ./config/webpack.config.js",
    "start": "webpack-dev-server --open --mode development --config ./config/webpack.config.js",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.js' -i 'src/**/*.jsx' -x 'src/test/**' -t json --threshold 90",
    "flow:badge": "flow-coverage-report -i 'src/**/*.js' -i 'src/**/*.jsx' -x 'src/test/**' -t badge --threshold 90",
    "flow:check": "flow check",
    "flow:type": "flow-typed install",
    "flow:__focus-check": "flow focus-check",
    "precommit": "yarn lint:staged",
    "lint": "eslint src/",
    "lint:__eslint": "eslint --fix",
    "lint:__prettier": "prettier-eslint --write --eslint-config-path \".eslintrc.json\" --print-width 80",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:__prettier",
      "yarn lint:__eslint",
      "yarn flow:__focus-check",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
